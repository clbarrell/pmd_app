<% provide(:title, "User List") %>
<h1>All Users</h1>
<div class="container">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <table class="table table-hover">
       <thead>
        <tr>
           <th>Name</th>
           <th>Email</th>
           <th>Initials</th>
           <th>Admin?</th>
        </tr>
       </thead>
       <tbody>
        <%= render @users %>
       </tbody>
      </table>
    </div>
  </div>
</div>