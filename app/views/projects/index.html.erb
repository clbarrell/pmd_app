<% provide(:title, "Project List") %>
<h1>All Projects</h1>
<div class="container-fluid">
  <div class="row">
    <div class="col-md-10 col-md-offset-1">
      <table class="table table-hover">
       <thead>
        <tr>
           <th>Job #</th>
           <th>Name</th>
           <th>Status</th>
           <th>Type</th>
           <th>Client</th>
           <th>Contact</th>
           <th>Probability</th>
           <th>Service line</th>
           <th>Total $</th>
           <th>Edit</th>
        </tr>
       </thead>
       <tbody>
        <%= render @projects %>
       </tbody>
      </table>
    </div>
  </div>
</div>